{
  "vim.insertModeKeyBindings": [],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "f"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "s",
      ],
      "when": "editorFocus",
    },
    {
      "before": [
        "<C-k>",
      ],
      "after": [
        "{",
      ],
      "when": "editorFocus",
    },
    {
      "before": [
        "<C-j>",
      ],
      "after": [
        "}",
      ],
      "when": "editorFocus",
    },
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-k>",
      ],
      "commands": [
        "list.focusUp",
      ],
      "when": "listFocus && !inputFocus",
    },
    {
      "before": [
        "k",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        ",",
      ],
      "after": [
        ";",
      ]
    },
    {
      "before": [
        ";",
      ],
      "after": [
        ",",
      ]
    },
    {
      "before": [
        "z",
        "z"
      ],
      "commands": [
        "editor.foldAll",
      ]
    },
    {
      "before": [
        "z",
        "r"
      ],
      "commands": [
        "editor.unfoldAll",
      ]
    },
    {
      "before": [
        "z",
        "a"
      ],
      "commands": [
        "editor.foldRecursively",
      ]
    },
    {
      "before": [
        "z",
        "m"
      ],
      "commands": [
        "editor.unfoldRecursively",
      ]
    },
    {
      "before": [
        "/"
      ],
      "commands": [
        "actions.find",
      ]
    },
    {
      "before": [
        ":"
      ],
      "commands": [
        "workbench.action.showCommands",
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "workbench.action.quickOpen",
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        {
          "command": "editor.action.marker.next",
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "c"
      ],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": [
            "cargo check"
          ],
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "i"
      ],
      "commands": [
        "editor.action.showHover",
      ]
    },
    {
      "before": [
        "<leader>",
        "j"
      ],
      "commands": [
        "workbench.action.gotoSymbol",
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.files.save",
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-j>": true,
    "<C-k>": true,
    "<C-a>": false,
    "<C-e>": false,
    "<C-w>": false,
    "<C-s>": false,
    "<C-n>": false,
    "<C-v>": false,
    "Â¨": false,
    "}": false,
    "<C-f>": false
  },
  "vim.easymotion": true,
  "vim.easymotionMarkerFontSize": "17",
  "vim.easymotionMarkerFontWeight": "bold",
  // "vim.easymotionMarkerBackgroundColor": "#ffffff",
  // "vim.easymotionMarkerForegroundColorOneChar": "#aa0000",
  "vim.visualstar": true,
  "vim.sneak": false,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": false,
  "editor.highlightActiveIndentGuide": true,
  "editor.renderIndentGuides": false,
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0,
  "workbench.startupEditor": "newUntitledFile",
  // "go.autocompleteUnimportedPackages": true,
  // "go.formatTool": "goimports",
  "editor.fontSize": 16,
  "editor.fontFamily": "'Fira Code', 'Menlo', 'Hack', 'Monaco', 'DejaVu Sans Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "vim.leader": "<space>",
  "editor.renderControlCharacters": false,
  "breadcrumbs.enabled": false,
  "editor.renderWhitespace": "none",
  // "go.useLanguageServer": true,
  "workbench.colorCustomizations": {
    "statusBar.noFolderBackground": "#BF616A",
    "editorLineNumber.activeForeground": "#5fd45f",
    "statusBar.debuggingBackground": "#BF616A",
    "editorBracketMatch.border": "#5fd45f",
    // "statusBar.background": "#6199bf",
  },
  "editor.tokenColorCustomizations": {
    "functions": "#C586C0",
    "keywords": "#569CD6",
    "types": "#C586C0",
  },
  "workbench.view.alwaysShowHeaderActions": true,
  "editor.formatOnSave": true,
  "terminal.integrated.fontSize": 18,
  // "rest-client.enableTelemetry": false,
  // "rest-client.timeoutinmilliseconds": 20000,
  "editor.autoClosingBrackets": "beforeWhitespace",
  "outline.problems.enabled": false,
  "window.menuBarVisibility": "toggle",
  "rust.build_on_save": true,
  "rust.jobs": 2,
  "[clojure]": {
    "editor.matchBrackets": true
  },
  "debug.console.fontSize": 16,
  "calva.lintOnSave": true,
  "workbench.colorTheme": "Default Light+"
}
