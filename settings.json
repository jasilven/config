{
  "vim.insertModeKeyBindings": [],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<alt>",
        "j"
      ],
      "after": [
        "}",
      ],
      "when": "editorFocus"
    },
    {
      "before": [
        "f"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "s"
      ],
      "when": "editorFocus"
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<alt>",
        "j"
      ],
      "after": [
        "}",
      ]
    },
    {
      "before": [
        "g",
        "m"
      ],
      "after": [
        "%"
      ]
    },
    {
      "before": [
        "g",
        "k"
      ],
      "after": [
        "{"
      ]
    },
    {
      "before": [
        "g",
        "j"
      ],
      "after": [
        "}"
      ]
    },
    {
      "before": [
        "g",
        "h"
      ],
      "commands": [
        "cursorHome"
      ]
    },
    {
      "before": [
        "g",
        "o"
      ],
      "commands": [
        "workbench.action.focusLastEditorGroup"
      ]
    },
    {
      "before": [
        "g",
        "l"
      ],
      "commands": [
        "cursorEnd"
      ]
    },
    {
      "before": [
        "R"
      ],
      "after": []
    },
    {
      "when": "calva:activated",
      "before": [
        "<leader>",
        "c",
        "c"
      ],
      "commands": [
        "calva.evaluateCurrentTopLevelForm"
      ]
    },
    {
      "when": "calva:activated",
      "before": [
        "<leader>",
        "c",
        "k"
      ],
      "commands": [
        "calva.loadFile"
      ]
    },
    {
      "when": "calva:activated",
      "before": [
        "<leader>",
        "c",
        "i"
      ],
      "commands": [
        "calva.evaluateSelectionAsComment"
      ]
    },
    {
      "when": "editorTextFocus && !editorHasMultipleSelections && !editorReadOnly && !hasOtherSuggestions && !suggestWidgetVisible && editorLangId == 'clojure'",
      "before": [
        "`"
      ],
      "commands": [
        "calva.clearInlineResults"
      ]
    },
    {
      "when": "calva:activated",
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "calva.evaluateSelection"
      ]
    },
    {
      "before": [
        ":"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "f1"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "j"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-r>": true,
    "<C-c>": false,
    "<C-k>": false,
    "<C-x>": false,
    "<C-a>": false,
    "<C-e>": false,
    "<C-w>": false,
    "<C-s>": false,
    "<C-n>": false,
    "<C-v>": false,
    "<C-f>": false
  },
  "explorer.confirmDelete": false,
  "debug.console.fontSize": 14,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.easymotionKeys": "hklyuopnm,qwertzxcvbasdgjf",
  "vim.easymotionMarkerFontSize": "17",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerFontFamily": "Fira Code",
  "vim.easymotionMarkerHeight": 1,
  "vim.easymotionMarkerYOffset": 13,
  "vim.easymotionMarkerForegroundColorOneChar": "#E92323",
  "vim.easymotionMarkerForegroundColorTwoChar": "#E92323",
  "vim.visualstar": true,
  "vim.sneak": false,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": false,
  "vim.hlsearch": false,
  "editor.highlightActiveIndentGuide": true,
  "editor.renderIndentGuides": false,
  "editor.fontSize": 16,
  "editor.renderControlCharacters": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.formatOnSave": true,
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.formatOnPaste": true,
  "editor.hover.delay": 400,
  "editor.maxTokenizationLineLength": 130000,
  "editor.fontFamily": "Fira Code Medium, Menlo, 'Courier New', monospace",
  "editor.occurrencesHighlight": false,
  "editor.hover.sticky": false,
  "editor.tokenColorCustomizations": {
    "[Solarized Dark]": {
      "numbers": "#dbdbdb",
      "keywords": "#859900",
      "types": "#839400",
      "variables": "#839400",
      "textMateRules": [
        {
          "scope": "string.regexp.clojure",
          "settings": {
            "foreground": "#aabb0f"
          }
        }
      ]
    },
  },
  "calva.highlight.enableBracketColors": false,
  "calva.fmt.removeConsecutiveBlankLines": true,
  "calva.highlight.ignoredFormStyle": {
    "textDecoration": "none; opacity: 0.5"
  },
  "window.zoomLevel": 0,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.tree.indent": 20,
  "workbench.editor.showTabs": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorCustomizations": {
    "editor.foreground": "#839496",
    // "editorBracketMatch.background": "#993399",
    "editor.inactiveSelectionBackground": "#FFFFFF",
  },
  "workbench.colorTheme": "Default Light+",
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "terminal.integrated.fontSize": 14,
  "[Log]": {
    "editor.fontSize": 12
  },
  "outline.problems.enabled": false,
  "[clojure]": {
    "editor.matchBrackets": true
  },
  "extensions.showRecommendationsOnlyOnDemand": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
}