{
    "rust.clippy_preference": "on",
    "coc-actions.showActionKind": true ,
  "rust-analyzer.serverPath": "~/.cargo/bin/rust-analyzer",
  "rust-analyzer.diagnostics.enable": true,
  "json.enable": true,
  "codeLens.enable":true,
  "codeLens.separator": "â€£",
  "json.format.enable": true,
  "coc.preferences.formatOnType": true,
  "coc.source.buffer.ignoreGitignore": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "clojure",
    "go",
    "json"
  ],
  "coc.preferences.snippets.enable": true,
  "diagnostic.enableSign": true,
  "diagnostic.joinMessageLines": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualText": true,
  "diagnostic.infoSign": "i",
  "diagnostic.errorSign": "!",
  "diagnostic.warningSign": "?",
  "diagnostic.highlightOffset": 1000,
  "diagnostic.hintSign": "i",
  "diagnostic.refreshOnInsertMode": true,
  "suggest.autoTrigger": "always",
  "suggest.snippetIndicator": "~",
  "suggest.labelMaxLength": 60,
  "suggest.maxPreviewWidth": 60,
  "suggest.detailMaxLength": 60,
  "suggest.enablePreview": true,
  "suggest.disableKind": false,
  "suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "deps.edn"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
